[user]
  name = Patrick Boe
  email = <%= print("Your Email: "); STDOUT.flush; STDIN.gets.chomp %>
[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  df = diff
  lg = log -p
  pom = push origin master
[color]
  ui = true
[core]
  excludesfile = <%= ENV['HOME'] %>/.gitignore
  editor = vim
  autocrlf = input
[apply]
  whitespace = nowarn
[format]
  pretty = %C(yellow)%h%Creset %s %C(red)(%an, %cr)%Creset
[diff]
  tool = vimdiff
[difftool "mvimdiff"]
  cmd = "mvimdiff -diff  "
[difftool]
  prompt = false
